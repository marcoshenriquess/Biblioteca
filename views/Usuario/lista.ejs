<h1 class="text-center text-black-50 pt-5">Lista Usuarios</h1>
<table class="table table-striped ">
  <div class="row mx-5 mt-5">
    <thead>
      <tr>
          <th>Id</th>
          <th>Nome</th>
          <th>Email</th>
          <th>CPF</th>
          <th>Ações</th>
      </tr>
  </thead>
  <tbody id="corpoTabela">
      <% const responses = lista %>
      <% lista.forEach(function(value, index) { %>
              <tr>
                  <td><%= value.cod %></td>
                  <td><%= value.nome %></td>
                  <td><%= value.email %></td>
                  <td><%= value.cpf %></td>
                  <td>
                    <div>
                      <button data-id="<%= value.usuarioCod %>" title="Excluir" class="btn btn-danger btnExcluir"><i class="fas fa-trash"></i></button>
                      <a href="/usuarios/alterar/<%= value.usuarioId %>" title="Editar" class="btn btn-primary">
                          <i class="fas fa-pen"></i>
                      </a>
                    </div>
                  </td>
              </tr>   
          <% }) %>        
  </tbody>
    <hr>
  </div>
</table>